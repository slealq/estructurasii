#!/bin/sh

rm simulation_output.csv # clean simulation csv
parallel --verbose ::: "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp LRU --mp 2 --csv mcf" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp LRU --mp 2 --csv mcf" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp LRU --mp 2 --csv mcf" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp LRU --mp 2 --csv mcf" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp LRU --mp 2 --csv art" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp LRU --mp 2 --csv art" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp LRU --mp 2 --csv art" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp LRU --mp 2 --csv art" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp NRU --mp 2 --csv mcf" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp NRU --mp 2 --csv mcf" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp NRU --mp 2 --csv mcf" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp NRU --mp 2 --csv mcf" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp NRU --mp 2 --csv art" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp NRU --mp 2 --csv art" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp NRU --mp 2 --csv art" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp NRU --mp 2 --csv art" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp SRRIP --mp 2 --csv mcf -m 2" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp SRRIP --mp 2 --csv mcf -m 2" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp SRRIP --mp 2 --csv mcf -m 2" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp SRRIP --mp 2 --csv mcf -m 2" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp SRRIP --mp 2 --csv art -m 2" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp SRRIP --mp 2 --csv art -m 2" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp SRRIP --mp 2 --csv art -m 2" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp SRRIP --mp 2 --csv art -m 2" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp SRRIP --mp 2 --csv mcf -m 3" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp SRRIP --mp 2 --csv mcf -m 3" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp SRRIP --mp 2 --csv mcf -m 3" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp SRRIP --mp 2 --csv mcf -m 3" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp SRRIP --mp 2 --csv art -m 3" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp SRRIP --mp 2 --csv art -m 3" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp SRRIP --mp 2 --csv art -m 3" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp SRRIP --mp 2 --csv art -m 3" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp SRRIP --mp 2 --csv mcf -m 4" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp SRRIP --mp 2 --csv mcf -m 4" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp SRRIP --mp 2 --csv mcf -m 4" "gunzip -c mcf.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp SRRIP --mp 2 --csv mcf -m 4" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 2 -l 16 --rp SRRIP --mp 2 --csv art -m 4" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 4 -l 16 --rp SRRIP --mp 2 --csv art -m 4" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 8 -l 16 --rp SRRIP --mp 2 --csv art -m 4" "gunzip -c art.trace.gz | ./cache.exe -t 128 -a 16 -l 16 --rp SRRIP --mp 2 --csv art -m 4"

